{
    "name": "svelte-slidy-site",
    "version": "2.4.9",
    "description": "Starter for new Svelte application with ESBuild bundler ",
    "main": "index.js",
    "scripts": {
        "build": "node esbuild",
        "dev": "node esbuild --dev",
        "start": "npx derver --production --port=5000 public",
        "predeploy": "node esbuild",
        "deploy": "gh-pages -d public"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/AlexxNB/svelte-esbuild-starter.git"
    },
    "keywords": [
        "svelte",
        "starter",
        "derver",
        "esbuild"
    ],
    "author": "Alexey Schebelev",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/AlexxNB/svelte-esbuild-starter/issues"
    },
    "homepage": "https://github.com/AlexxNB/svelte-esbuild-starter#readme",
    "devDependencies": {
        "derver": "^0.4.13",
        "esbuild": "^0.9.2",
        "esbuild-svelte": "^0.4.3",
        "esbuild-svelte-paths": "^0.1.3",
        "gh-pages": "^3.1.0",
        "sass": "^1.32.8",
        "svelte": "^3.35.0",
        "svelte-preprocess": "^4.6.9",
        "svelte-reactive-preprocessor": "^0.6.1"
    }
}