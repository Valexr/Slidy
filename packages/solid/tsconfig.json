{
    "extends": "../../tsconfig.json",
    "compilerOptions": {
        "declarationDir": "dist",
        "jsx": "preserve",
        "jsxImportSource": "solid-js",
        "allowSyntheticDefaultImports": true,
        "sourceMap": true,
        "skipLibCheck": true,
        "preserveValueImports": true
    },
    "include": [
        "src/*",
        "src/dev/*"
    ],
    "exclude": [
        "dist",
        "public"
    ]
}